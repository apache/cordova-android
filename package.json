        "q": "^1.4.1",
        "shelljs": "^0.5.3"
    },
    "bundledDependencies": [
        "cordova-common",
        "elementtree",
        "nopt",
        "properties-parser",
        "q",
        "shelljs"
    ],
    "devDependencies": {
        "jasmine-node": "^1.14.5",
