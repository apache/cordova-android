{
    "name": "cordova-android",
    "version": "5.0.0-dev",
    "description": "cordova-android release",
    "bin": {
        "create": "bin/create"
    },
    "main": "bin/templates/cordova/Api.js",
    "repository": {
        "type": "git",
        "url": "https://git-wip-us.apache.org/repos/asf/cordova-android.git"
    },
